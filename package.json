{
	"name": "rokameal",
	"version": "1.0.3",
	"author": {
		"email": "me@esinx.net",
		"name": "esinx",
		"url": "esinx.net"
	},
	"homepage": "https://github.com/esinx/rokameal",
	"repository": {
		"type": "git",
		"url": "https://github.com/esinx/rokameal.git"
	},
	"description": "Simplified meal data fetcher using Republic of Korea Army Open Data API (Ministry of Defense)",
	"keywords": [
		"ROKA",
		"meal",
		"open-data",
		"mnd"
	],
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"/dist"
	],
	"scripts": {
		"test": "jest",
		"build": "tsc",
		"prepublishOnly": "tsc",
		"lint": "yarn lint:script",
		"lint:script": "eslint ./src --ext .js,.jsx,.ts,.tsx"
	},
	"dependencies": {
		"cross-fetch": "^3.1.5",
		"luxon": "^3.0.1"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^5.0.1",
		"@semantic-release/commit-analyzer": "^8.0.1",
		"@semantic-release/git": "^9.0.0",
		"@semantic-release/npm": "^7.0.8",
		"@semantic-release/release-notes-generator": "^9.0.1",
		"@types/jest": "^28.1.6",
		"@types/luxon": "^3.0.0",
		"@typescript-eslint/parser": "^5.27.0",
		"dotenv": "^16.0.1",
		"eslint": "^8.16.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"jest": "^28.1.3",
		"prettier": "^2.7.1",
		"semantic-release": "^17.3.0",
		"ts-jest": "^28.0.7",
		"ts-node": "^10.8.0",
		"tsc": "^2.0.4",
		"tsconfig-paths": "^4.0.0",
		"typescript": "^4.7.2"
	},
	"jest": {
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"testMatch": [
			"**/*.test.ts"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"json"
		]
	},
	"release": {
		"branches": [
			"main",
			"next"
		],
		"preset": "conventionalcommits",
		"plugins": [
			[
				"@semantic-release/commit-analyzer",
				{
					"releaseRules": [
						{
							"type": "revert",
							"release": "patch"
						},
						{
							"type": "build",
							"release": "patch"
						}
					]
				}
			],
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/changelog",
				{
					"changelogTitle": "# Changelog"
				}
			],
			[
				"@semantic-release/npm",
				{
					"pkgRoot": "dist"
				}
			],
			[
				"@semantic-release/git",
				{
					"message": "chore(release): ${nextRelease.version} [skip ci]",
					"assets": [
						"CHANGELOG.md"
					]
				}
			]
		]
	}
}
